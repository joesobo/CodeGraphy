{
  "name": "codegraphy",
  "displayName": "CodeGraphy",
  "description": "Graphical file interface",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.67.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "publisher": "codegraphy",
  "author": {
    "name": "Joe Soboleski",
    "email": "soboleskijoe@gmail.com"
  },
  "homepage": "https://github.com/joesobo/CodeGraphy",
  "repository": {
    "type": "git",
    "url": "https://github.com/joesobo/CodeGraphy.git"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "galleryBanner": {
    "color": "#023859",
    "theme": "dark"
  },
  "keywords": [
    "code",
    "graph",
    "codegraphy",
    "codegraph",
    "graphy",
    "connection",
    "physics",
    "node",
    "js",
    "ts"
  ],
  "main": "./dist/extension.js",
  "icon": "img/graph.png",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "concurrently \"rollup -c\" \"webpack\"",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts"
  },
  "activationEvents": [
    "onView:code-graphy-sidebar",
    "onCommand:codegraphy.graphCode"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "code-graphy-sidebar-view",
          "title": "CodeGraphy",
          "icon": "img/graph.png"
        }
      ]
    },
    "views": {
      "code-graphy-sidebar-view": [
        {
          "type": "webview",
          "id": "code-graphy-sidebar",
          "name": "CodeGraphy",
          "icon": "img/graph.png",
          "contextualTitle": "CodeGraphy"
        }
      ]
    },
    "commands": [
      {
        "command": "codegraphy.graphCode",
        "category": "CodeGraphy",
        "title": "Graph Code"
      }
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/cytoscape": "^3.19.4",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "concurrently": "^7.2.1",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "rollup": "^2.3.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "cose-base": "^2.1.0",
    "cytoscape": "^3.21.1",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-fcose": "^2.1.0",
    "cytoscape-popper": "^2.0.0",
    "cytoscape-qtip": "^2.8.0",
    "jquery": "^3.6.0"
  }
}
