{
	"name": "codegraphy",
	"displayName": "CodeGraphy",
	"description": "Graphical file interface",
	"version": "0.8.1",
	"engines": {
		"vscode": "^1.67.0"
	},
	"license": "SEE LICENSE IN LICENSE",
	"publisher": "codegraphy",
	"author": {
		"name": "Joe Soboleski",
		"email": "soboleskijoe@gmail.com"
	},
	"homepage": "https://github.com/joesobo/CodeGraphy",
	"repository": {
		"type": "git",
		"url": "https://github.com/joesobo/CodeGraphy.git"
	},
	"categories": [
		"Visualization",
		"Other"
	],
	"galleryBanner": {
		"color": "#023859",
		"theme": "dark"
	},
	"keywords": [
		"code",
		"graph",
		"codegraphy",
		"codegraph",
		"graphy",
		"connection",
		"physics",
		"node",
		"js",
		"ts"
	],
	"main": "./dist/extension.js",
	"icon": "img/graph.png",
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "concurrently \"rollup -c\" \"webpack\" \"vite build\" \"tailwindcss -i ./src/assets/tailwind.css -o ./dist/output.css\"",
		"watch": "concurrently \"rollup -c -w\" \"webpack --watch\" \"vite build --watch\" \"tailwindcss -i ./src/assets/tailwind.css -o ./dist/output.css --watch\"",
		"package": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext .js,.vue,.ts,.json",
		"format": "prettier --write .",
		"test": "jest"
	},
	"activationEvents": [
		"onView:code-graphy-sidebar",
		"onCommand:codegraphy.graphCode"
	],
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "code-graphy-sidebar-view",
					"title": "CodeGraphy",
					"icon": "img/graph.png"
				}
			]
		},
		"views": {
			"code-graphy-sidebar-view": [
				{
					"type": "webview",
					"id": "code-graphy-sidebar",
					"name": "CodeGraphy",
					"icon": "img/graph.png",
					"contextualTitle": "CodeGraphy"
				}
			]
		},
		"commands": [
			{
				"command": "codegraphy.graphCode",
				"category": "CodeGraphy",
				"title": "Graph Code"
			}
		],
		"configuration": {
			"title": "CodeGraphy Settings",
			"properties": {
				"codegraphy.selectedColor": {
					"type": "string",
					"default": [
						"#fff"
					],
					"description": "Selected Node Color"
				},
				"codegraphy.nodeSettings": {
					"type": "array",
					"default": [
						{
							"extension": ".js",
							"color": "#4985be"
						}
					],
					"description": "Node Metadata"
				},
				"codegraphy.blacklist": {
					"type": "array",
					"default": [
						".vscode",
						".git",
						".github",
						"node_modules"
					],
					"description": "Blacklist folders and files"
				}
			}
		}
	},
	"devDependencies": {
		"@babel/core": "^7.18.13",
		"@babel/preset-env": "^7.18.10",
		"@babel/preset-typescript": "^7.18.6",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@rollup/plugin-typescript": "^8.1.0",
		"@testing-library/vue": "^6.6.1",
		"@types/cytoscape": "^3.19.4",
		"@types/glob": "^7.2.0",
		"@types/jest": "^29.0.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"@vitejs/plugin-vue": "^2.3.3",
		"@vscode/test-electron": "^2.1.3",
		"babel-jest": "^29.0.2",
		"concurrently": "^7.2.1",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-vue": "^9.4.0",
		"glob": "^8.0.1",
		"jest": "^29.0.2",
		"mocha": "^9.2.2",
		"prettier": "2.7.1",
		"rollup": "^2.3.4",
		"rollup-plugin-terser": "^7.0.2",
		"tailwindcss": "^3.1.8",
		"ts-jest": "^28.0.8",
		"ts-loader": "^9.2.8",
		"tsc": "^2.0.4",
		"typescript": "^4.6.4",
		"vite": "^2.9.14",
		"vue-eslint-parser": "^9.0.3",
		"vue-tsc": "^0.40.6",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.5",
		"cose-base": "^2.1.0",
		"cytoscape": "^3.21.1",
		"cytoscape-cola": "^2.5.1",
		"cytoscape-cose-bilkent": "^4.1.0",
		"cytoscape-fcose": "^2.1.0",
		"cytoscape-popper": "^2.0.0",
		"cytoscape-qtip": "^2.8.0",
		"jquery": "^3.6.0",
		"vue": "^3.2.37",
		"vue-color-input": "^1.0.6"
	}
}
